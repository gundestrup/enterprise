
<% form_for(@feed) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
	<p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows=> "5", :cols=> "22" %>
  </p>
  
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>